{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2022", "DOM"],
      "module": "ESNext",
      "moduleResolution": "NodeNext",
      "outDir": "./.vercel/output/functions",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "types": ["node"],
      "sourceMap": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["api/**/*.ts"],
    "exclude": ["node_modules"]
  }